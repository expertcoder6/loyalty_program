<h1>Transactions</h1>
<table>
	<tr>
		<th>ID</th>
		<th>Name</th>
		<th>Amount</th>
	</tr>
	<% @transactions.each do |transaction| %>
		<tr>
			<td><%= transaction.id %>&nbsp;</td>
			<td><%= Product.find(transaction.product_id).name %></td>
			<td><%=transaction.amount%></td>
		</tr>
	<% end %>
</table>

<%= link_to "Back",root_path %>